.nav-menu li:hover {
  background-color: #164a84;
  z-index:999;
  -webkit-transform: scale(1.1); 
  transform: scale(1.1);  
}

.nav-menu li:hover .nav-icon {
  font-size: 60px;
  opacity:0.9;
}

.nav-menu li:hover .nav-title {
  visibility: hidden;
}

.nav-menu-login li:hover {
  background-color: #164a84;
  z-index:999;
  -webkit-transform: scale(1.1); 
  transform: scale(1.1);  
}

.nav-menu-login li:hover .nav-icon-login {
  font-size: 60px;
  opacity:0.9;
}

.nav-menu-login li:hover .nav-title-login {
  visibility: hidden;
}

