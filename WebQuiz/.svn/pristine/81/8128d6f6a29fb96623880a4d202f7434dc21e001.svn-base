  .message-bubble {
    position: absolute;
	top:0px;
	left:120px;
    width: 1050px;
    height: 450px;
    padding: 0px;
    background: #ADB400;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    border-radius: 10px;
	display: none;
	z-index: 200;
	over-flow: hidden;
}

  .message-bubble:after {
    content: "";
    position: absolute;
    top: 120px;
    left: -16px;
    border-style: solid;
    border-width: 14px 16px 14px 0;
    border-color: transparent #ADB400;
    display: block;
    width: 0;
    z-index: 200;
}

.achieve-bubble {
    position: absolute;
	top:0px;
	left:120px;
    width: 1050px;
    height: 450px;
    padding: 0px;
    background: #AAAAFF;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    border-radius: 10px;
	display: none;
	z-index: 200;
}

  .achieve-bubble:after {
    content: "";
    position: absolute;
    top: 390px;
    left: -16px;
    border-style: solid;
    border-width: 14px 16px 14px 0;
    border-color: transparent #AAAAFF;
    display: block;
    width: 0;
    z-index: 200;
}

.achieve-bubble ul{
	list-style-type: none;
}

